<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FinalProjectTakeSix.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FinalProjectTakeSix"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Sign Up"
        ContentTemplate="{DataTemplate local:SignUp}"
        Route="SignUp" />

    <ShellContent
        Title="Log In"
        ContentTemplate="{DataTemplate local:LogIn}"
        Route="LogIn" />

    <ShellContent
        Title="ManageMember"
        ContentTemplate="{DataTemplate local:ManageMember}"
        Route="ManageMember" />

</Shell>
